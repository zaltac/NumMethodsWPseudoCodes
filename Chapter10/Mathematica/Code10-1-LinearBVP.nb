Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"LBVPSolve", "[", 
    RowBox[{
    "a0_", ",", "b0_", ",", "n0_", ",", "\[Alpha]_", ",", "\[Beta]_", ",", 
     "\[Gamma]_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"A", "=", 
        RowBox[{"N", "[", "a0", "]"}]}], ",", 
       RowBox[{"B", "=", 
        RowBox[{"N", "[", "b0", "]"}]}], ",", 
       RowBox[{"n", "=", "n0"}], ",", "a", ",", " ", "b", ",", " ", "c", ",", 
       " ", "d", ",", " ", "h", ",", " ", "pxk", ",", "qxk", ",", "rxk", ",", 
       " ", "fxk", ",", " ", "i", ",", " ", "k"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"h", "=", 
       FractionBox[
        RowBox[{"B", "-", "A"}], "n"]}], ";", 
      RowBox[{"neq", "=", 
       RowBox[{"n", "+", "1"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"BCLeft", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Alpha]", 
           RowBox[{"\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]], 
          "\[Equal]", "0"}], ",", "0", ",", "1"}], "]"}]}], ";", " ", 
      "\[IndentingNewLine]", 
      RowBox[{"BCRigt", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Alpha]", 
           RowBox[{"\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]], 
          "\[Equal]", "0"}], ",", "0", ",", "1"}], "]"}]}], ";", " ", 
      "\[IndentingNewLine]", 
      RowBox[{"a", "=", 
       RowBox[{"b", "=", 
        RowBox[{"c", "=", 
         RowBox[{"d", "=", 
          RowBox[{"x", "=", 
           RowBox[{"Table", "[", 
            RowBox[{"0", ",", 
             RowBox[{"{", "neq", "}"}]}], "]"}]}]}]}]}]}], ";", " ", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"k", "=", "1"}], ",", 
        RowBox[{"k", "\[LessEqual]", "neq"}], ",", 
        RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          SubscriptBox["x", 
           RowBox[{"\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}]], 
          "=", 
          RowBox[{"A", "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"k", "-", "1"}], ")"}], " ", "h"}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"foundc", "=", 
          RowBox[{"COEFFS", "[", 
           SubscriptBox["x", 
            RowBox[{"\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}]], 
           "]"}]}], ";", " ", "\[IndentingNewLine]", 
         RowBox[{"pxk", "=", 
          RowBox[{
          "foundc", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
         ";", 
         RowBox[{"qxk", "=", 
          RowBox[{
          "foundc", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"rxk", "=", 
          RowBox[{
          "foundc", "\[LeftDoubleBracket]", "3", "\[RightDoubleBracket]"}]}], 
         ";", 
         RowBox[{"fxk", "=", 
          RowBox[{
          "foundc", "\[LeftDoubleBracket]", "4", "\[RightDoubleBracket]"}]}], 
         ";", " ", "\[IndentingNewLine]", 
         RowBox[{
          SubscriptBox["d", 
           RowBox[{"\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}]], 
          "=", 
          RowBox[{"(", 
           RowBox[{"rxk", "-", 
            RowBox[{"2", " ", 
             FractionBox["pxk", 
              SuperscriptBox["h", "2"]]}]}], ")"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
          SubscriptBox["a", 
           RowBox[{"\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}]], 
          "=", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             FractionBox["pxk", "h"], "+", " ", 
             FractionBox["qxk", "2"]}], ")"}], 
           FractionBox["1", "h"]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{
          SubscriptBox["b", 
           RowBox[{"\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}]], 
          "=", 
          RowBox[{
           RowBox[{"(", 
