Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"TriDiagonal", "[", 
     RowBox[{
     "s1_", ",", "sn_", ",", "b0_", ",", " ", "d0_", ",", " ", "a0_", ",", 
      " ", "c0_"}], "]"}], " ", ":=", "\n", "  ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", " ", 
       RowBox[{
        RowBox[{"a", " ", "=", " ", "a0"}], ",", " ", 
        RowBox[{"b", " ", "=", " ", "b0"}], ",", " ", 
        RowBox[{"c", " ", "=", " ", "c0"}], ",", " ", 
        RowBox[{"d", " ", "=", " ", "d0"}]}], " ", "}"}], ",", "\n", "   ", 
      RowBox[{
       RowBox[{"neq", "=", 
        RowBox[{"sn", "-", "s1", "+", "1"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"Forward", " ", "Elimination", " ", "step"}], " ", "*)"}], 
       " ", "\[IndentingNewLine]", 
       RowBox[{"For", "[", " ", 
        RowBox[{
         RowBox[{"i", " ", "=", 
          RowBox[{"s1", "+", "1"}]}], ",", " ", 
         RowBox[{"i", " ", "\[LessEqual]", "sn"}], ",", " ", 
         RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"R", "=", 
           RowBox[{
            SubscriptBox["b", 
             RowBox[{"\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]], 
            "/", 
            SubscriptBox["d", 
             RowBox[{"\[LeftDoubleBracket]", 
              RowBox[{"i", "-", "1"}], "\[RightDoubleBracket]"}]]}]}], ";", 
          "\n", "  ", 
          RowBox[{
           SubscriptBox["d", 
            RowBox[{"\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]], 
           "=", 
           RowBox[{
            SubscriptBox["d", 
             RowBox[{"\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]], 
            "-", 
            RowBox[{"R", "*", 
             SubscriptBox["a", 
              RowBox[{"\[LeftDoubleBracket]", 
               RowBox[{"i", "-", "1"}], "\[RightDoubleBracket]"}]]}]}]}], ";",
           " ", "\n", "  ", 
          RowBox[{
           SubscriptBox["c", 
            RowBox[{"\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]], 
           "=", 
           RowBox[{
            SubscriptBox["c", 
             RowBox[{"\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]], 
            "-", 
            RowBox[{"R", "*", 
             SubscriptBox["c", 
              RowBox[{"\[LeftDoubleBracket]", 
               RowBox[{"i", "-", "1"}], "\[RightDoubleBracket]"}]]}]}]}], 
